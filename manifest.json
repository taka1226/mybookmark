{
  "name": "My Bookmarks",
  "version": "1.0",
  "description": "it allows you to group your bookmarks , add and delete whenever you want.",
  "permissions": [
    "bookmarks",
    "history",
    "storage",
    "tabs"
  ],
  "browser_action": {
      "default_title": "My Bookmarks",
      "default_icon": "pipi.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
